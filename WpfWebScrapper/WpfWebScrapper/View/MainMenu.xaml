<UserControl x:Class="WpfWebScrapper.View.V_MainMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfWebScrapper.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Button Content="Func1" Grid.Row="0" Style="{StaticResource btn_Default}"/>
        <Button Content="Func2" Grid.Row="1" Style="{StaticResource btn_Default}"/>
        <Button Content="Func3" Grid.Row="2" Style="{StaticResource btn_Default}"/>
    </Grid>
</UserControl>
